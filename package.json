{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.development craco start",
    "start": "env-cmd -f .env.development craco start",
    "start:development": "env-cmd -f .env.development craco start",
    "start:test": "env-cmd -f .env.test craco start",
    "start:release": "env-cmd -f .env.release craco start",
    "start:prod": "env-cmd -f .env.production craco start",
    "build:development": "env-cmd -f .env.development craco build",
    "build:test": "env-cmd -f .env.test craco build",
    "build:release": "env-cmd -f .env.release craco build",
    "build:prod": "env-cmd -f .env.production craco build",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,less,md}\" \"*.{js,ts,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,less,md}\" \"*.{js,ts,json,md}\"",
    "code:check": "npm run lint && npm run format:check",
    "code:fix": "npm run lint:fix && npm run format"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "antd": "^5.26.6",
    "axios": "^1.11.0",
    "i18next": "^25.3.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.6.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.7.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "craco-less": "^3.0.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "less": "^4.4.0",
    "less-loader": "^12.3.0",
    "msw": "^2.10.4",
    "prettier": "^3.6.2"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
